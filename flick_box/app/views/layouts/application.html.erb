<!DOCTYPE html>
<html>
<head>
  <title>FlickBox</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<% if current_user %>
  Signed in as <%= current_user.name %> | <%= link_to "Video Control Page", '/videos' %> | <%= link_to "Logout", '/logout' %>
<% else %>
  <%= link_to 'Login', '/login' %> | <%= link_to 'Signup', '/signup' %>
<% end %>

| <%= link_to 'Home', users_path %>

<%= yield %>

</body>
</html>
